 <div class="hover-table-layout">
   
  <% users.forEach(function(user,f){ %>
    <% var i=1;%>
    <% if(user.isAdmin == false && user.isSchoolAdmin == false){%>
     <div class="listing-item">
         <figure class="image">
             <img src="images/user.jpg" alt="image">
             <figcaption>
               <div class="caption">
                 <h1><%= user.username %></h1>
                 <p><%= user.education %></p>
                 </div>
             </figcaption>
         </figure>
         <div class="listing">
             
             <h4>Phone No: &nbsp;&nbsp;<%= user.phone %></h4>
             <h4>Email: <%= user.username %></h4>
             <h4>class:&nbsp;&nbsp;<%= user.education %></h4>
             <form action="/your_final_courses" method="POST">
             
                <input type="hidden" name="userid" value="<%=user._id %>">
                <button>CHECK</button> 
            </form>
        </div>
     </div>
     
     <%}%>
     <% i++; }) %>
  
</div>
